#include "version.hpp"
/////////////////////////////////////////////////////////////////////////////
//
// Version
//

1 VERSIONINFO
 FILEVERSION cryptonote_VERSION_WINDOWS_COMMA
 PRODUCTVERSION cryptonote_VERSION_WINDOWS_COMMA
 FILEFLAGSMASK 0x3fL
 FILEFLAGS 0x0L
 FILEOS 0x40004L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "LumenCoin Developers"
            VALUE "FileDescription", "LumenCoin"
            VALUE "FileVersion", cryptonote_VERSION_STRING
            VALUE "InternalName", "LumenCoin"
            VALUE "LegalCopyright", "Copyright (C) LumenCoin Developers 2018"
            VALUE "OriginalFilename", "LumenCoin"
            VALUE "ProductName", "LumenCoin"
            VALUE "ProductVersion", cryptonote_VERSION_STRING
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END

// Icon with lowest ID value placed first to ensure application icon
// remains consistent on all systems.
101               ICON                    "lum.ico"
